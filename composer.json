{
  "name": "teo_crm/yii2-fontawesome",
  "description": "Asset Bundle for Yii2 with Font Awesome",
  "keywords": [
    "yii",
    "font",
    "awesome",
    "asset",
    "bundle"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/Artem163397/yii2-fontawesome/issues",
    "source": "https://github.com/Artem163397/yii2-fontawesome"
  },
  "authors": [
    {
      "name": "Artem",
      "email": "artem.kovalskiy.93@mail.ru",
      "homepage": "http://shop-crm.ru/"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "2.0.*",
    "fortawesome/font-awesome": "~4.7"
  },
  "autoload": {
    "psr-4": {
      "teo_crm\\yii\\fontawesome\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
